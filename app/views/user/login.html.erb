<h2>Hello some user!</h2>
<%= form_tag({controller: "user", action: "login"}) do %>

    <%= label_tag(:username, "Username") %> : <%= text_field_tag(:username) %> <br><br>
    <%= label_tag(:password, "Password") %> : <%= password_field_tag(:password) %> <br><br>

    <%= submit_tag("Submit") %>
<% end %>




